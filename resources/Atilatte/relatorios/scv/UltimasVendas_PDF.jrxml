	<queryString>
		<![CDATA[SELECT AA80NA, AA80NOME, AA80BAIRRO, AA601NOME, AB80.AB80CUVDA, STATUS, 0 AS DIAS  FROM AB80 AB80 INNER JOIN AA80 ON AA80ID = AB80ENT INNER JOIN AA601 ON AA601ID = AA80MUNICIPIO LEFT JOIN AA80AUX ON AA80AUXFK = AA80ID LEFT JOIN AB80 AB80REP0 ON AB80REP0.AB80ID = AB80.AB80CREP0 LEFT JOIN AB80 AB80REP1 ON AB80REP1.AB80ID = AB80.AB80CREP1 LEFT JOIN AB80 AB80REP2 ON AB80REP2.AB80ID = AB80.AB80CREP2 LEFT JOIN AB80 AB80REP3 ON AB80REP3.AB80ID = AB80.AB80CREP3 LEFT JOIN AB80 AB80REP4 ON AB80REP4.AB80ID = AB80.AB80CREP4 WHERE AB80.AB80CLI = 1 AND ((AB80REP0.AB80CODIGO BETWEEN '0400001001' AND '0500008001' ) OR (AB80REP1.AB80CODIGO BETWEEN '0400001001' AND '0500008001' ) OR (AB80REP2.AB80CODIGO BETWEEN '0400001001' AND '0500008001' ) OR (AB80REP3.AB80CODIGO BETWEEN '0400001001' AND '0500008001' ) OR (AB80REP4.AB80CODIGO BETWEEN '0400001001' AND '0500008001' ))  AND (AB80.AB80CODIGO BETWEEN '0100001005' AND '9000001000' ) AND AB80.AB80EMPRESA  = 1322578 AND AB80.AB80CUVDA <= '2013/05/24' ORDER BY AA80NA]]>
	</queryString>
